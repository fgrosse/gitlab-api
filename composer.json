{
    "name": "fgrosse/gitlab-api",
    "description": "A guzzle 5 client for the gitlab API using service descriptions",
    "type": "library",
    "homepage": "https://github.com/fgrosse/gitlab-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Friedrich GroÃŸe",
            "email": "friedrich.grosse@gmail.com",
            "homepage": "https://github.com/FGrosse",
            "role": "Author"
        },
        {
            "name": "All contributors",
            "homepage": "https://github.com/FGrosse/PHPASN1/contributors"
        }
    ],
    "require": {
        "php": ">= 5.5",
        "symfony/yaml": "~2.6",
        "guzzlehttp/guzzle-services": "~0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "mockery/mockery": "dev-master as 1.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Gitlab\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gitlab\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
